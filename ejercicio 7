import math
def calcular_area_circulo(radio):

    area = math.pi * radio ** 2
    return area

radio1= 10
radio2= 5
radio3= 7

print("El area de un circulo con radio", radio1,"es:", calcular_area_circulo(radio1))
print("El area de un circulo con radio", radio2,"es:", calcular_area_circulo(radio2))
print("El area de un circulo con radio", radio3,"es:", calcular_area_circulo(radio3))
